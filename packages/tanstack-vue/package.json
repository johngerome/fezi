{
  "name": "@fezi/tanstack-vue",
  "version": "0.1.8",
  "description": "TanStack Query (Vue) integration for fezi",
  "author": "John Gerome Baldonado",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/johngerome/fezi.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "keywords": [
    "fetch",
    "http",
    "vue",
    "tanstack",
    "query",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc -b && tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@fezi/client": "workspace:*"
  },
  "devDependencies": {
    "@tanstack/vue-query": "^5.75.7",
    "@types/node": "^22.15.17",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-v8": "3.1.3",
    "jsdom": "^26.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0",
    "vitest": "^3.1.3",
    "zod": "^3.24.4"
  },
  "peerDependencies": {
    "@tanstack/vue-query": "^5.0.0",
    "vue": "^3.0.0",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
